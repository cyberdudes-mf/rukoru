<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hoshisugi.rukoru.app.view.content.EC2ContentController">
   <children>
      <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab fx:id="instanceTab" text="インスタンス">
            <content>
              <fx:include source="EC2InstanceTabView.fxml" />
            </content>
          </Tab>
          <Tab fx:id="amiTab" text="AMI">
            <content>
              <fx:include source="AMITabView.fxml" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</VBox>
