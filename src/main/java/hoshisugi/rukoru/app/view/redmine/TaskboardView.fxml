<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="layoutRoot" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hoshisugi.rukoru.app.view.redmine.TaskboardController">
   <children>
      <HBox maxWidth="1.7976931348623157E308" spacing="5.0">
         <children>
          <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButtonClicked" />
            <Button fx:id="prevButton" mnemonicParsing="false" onAction="#onPrevButtonClicked" />
            <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#onRefreshButtonClicked" />
         </children>
         <padding>
            <Insets bottom="5.0" right="5.0" />
         </padding>
      </HBox>
      <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <children>
            <ProgressIndicator maxHeight="50.0" maxWidth="50.0" prefHeight="50.0" prefWidth="50.0" />
         </children>
      </StackPane>
   </children>
</VBox>
