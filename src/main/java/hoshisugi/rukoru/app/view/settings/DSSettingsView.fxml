<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	nodeOrientation="RIGHT_TO_LEFT" xmlns="http://javafx.com/javafx/8.0.121"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="hoshisugi.rukoru.app.view.settings.DSSettingsController">
	<children>
		<ButtonBar prefHeight="40.0" prefWidth="200.0">
			<buttons>
				<Button fx:id="deleteButton" contentDisplay="RIGHT"
					mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete" />
				<Button fx:id="createButton" contentDisplay="RIGHT"
					mnemonicParsing="false" onAction="#onEntryButtonClick" text="Create" />
			</buttons>
			<padding>
				<Insets right="10.0" />
			</padding>
		</ButtonBar>
		<TableView fx:id="tableView" maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT"
			VBox.vgrow="ALWAYS">
			<columns>
				<TableColumn fx:id="nameColumn" editable="false"
					maxWidth="1.7976931348623157E308" minWidth="200.0" prefWidth="200.0"
					text="名前" />
				<TableColumn fx:id="dsHomeColumn" maxWidth="1.7976931348623157E308"
					minWidth="240.0" prefWidth="240.0" text="インストールディレクトリ">
				</TableColumn>
				<TableColumn fx:id="executionTypeColumn" maxWidth="1.7976931348623157E308"
					minWidth="80.0" resizable="false" text="起動方法">
				</TableColumn>
			</columns>
			<VBox.margin>
				<Insets left="10.0" right="10.0" />
			</VBox.margin>
		</TableView>
		<ButtonBar prefHeight="40.0" prefWidth="200.0"
			nodeOrientation="LEFT_TO_RIGHT">
			<buttons>
				<Button mnemonicParsing="false" onAction="#onApplyButtonClick"
					text="Apply" />
			</buttons>
			<padding>
				<Insets right="10.0" />
			</padding>
		</ButtonBar>
	</children>
</VBox>
