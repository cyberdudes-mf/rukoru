<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hoshisugi.rukoru.app.view.settings.DSSettingsController">
	<children>
      <HBox maxWidth="1.7976931348623157E308" spacing="5.0">
         <children>
				<Button fx:id="createButton" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onEntryButtonClick" />
				<Button fx:id="deleteButton" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onDeleteButtonClick" />
            <HBox nodeOrientation="RIGHT_TO_LEFT" HBox.hgrow="ALWAYS">
               <children>
                  <ToggleButton fx:id="showConsoleButton" mnemonicParsing="false" />
               </children>
            </HBox>
         </children>
      </HBox>
		<TableView fx:id="tableView" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
			<columns>
				<TableColumn fx:id="nameColumn" editable="true" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="170.0" text="名前">
					<cellValueFactory>
						<PropertyValueFactory property="name" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="executionPathColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="200.0" text="インストールディレクトリ">
					<cellValueFactory>
						<PropertyValueFactory property="executionPath" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="executionTypeColumn" editable="true" maxWidth="1.7976931348623157E308" minWidth="-1.0" resizable="false" text="起動方法">
					<cellValueFactory>
						<PropertyValueFactory property="executionType" />
					</cellValueFactory>
				</TableColumn>
            	<TableColumn fx:id="studioModeColumn" maxWidth="1.7976931348623157E308" minWidth="-1.0" prefWidth="120.0" text="Studio">
					<cellValueFactory>
						<PropertyValueFactory property="studioMode" />
					</cellValueFactory>
				</TableColumn>
			</columns>
			<VBox.margin>
				<Insets />
			</VBox.margin>
		</TableView>
		<ButtonBar prefHeight="40.0" prefWidth="200.0">
			<buttons>
				<Button mnemonicParsing="false" onAction="#onApplyButtonClick" text="Apply" />
			</buttons>
		</ButtonBar>
	</children>
   <padding>
      <Insets left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
