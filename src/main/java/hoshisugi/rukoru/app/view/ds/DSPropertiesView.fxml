<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hoshisugi.rukoru.app.view.ds.DSPropertiesController">
	<children>
		<SplitPane dividerPositions="0.25" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="500.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
			<items>
				<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
					<children>
						<TreeView fx:id="treeView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
					</children>
				</VBox>
				<VBox fx:id="layoutRoot" visible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
					<children>
						<ButtonBar buttonMinWidth="24.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0">
							<buttons>
								<Button fx:id="deleteButton" mnemonicParsing="false" />
								<Button fx:id="addButton" mnemonicParsing="false" />
							</buttons>
						</ButtonBar>
						<TableView fx:id="tableView" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
							<columns>
								<TableColumn fx:id="isEnableColumn" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" sortable="false" text="Enable">
									<cellValueFactory>
										<PropertyValueFactory property="enable" />
									</cellValueFactory>
								</TableColumn>
								<TableColumn fx:id="keyColumn" prefWidth="230.0" text="Key">
									<cellValueFactory>
										<PropertyValueFactory property="key" />
									</cellValueFactory>
								</TableColumn>
								<TableColumn fx:id="valueColumn" prefWidth="75.0" text="Value">
									<cellValueFactory>
										<PropertyValueFactory property="value" />
									</cellValueFactory>
								</TableColumn>
							</columns>
							<columnResizePolicy>
								<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
							</columnResizePolicy>
						</TableView>
                  <ButtonBar prefHeight="40.0" prefWidth="200.0">
                     <buttons>
                        <Button mnemonicParsing="false" onAction="#onApplyButtonClick" text="Apply" />
                     </buttons>
                  </ButtonBar>
					</children>
				</VBox>
			</items>
		</SplitPane>
		<ButtonBar prefHeight="40.0" prefWidth="200.0">
			<buttons>
            <Button mnemonicParsing="false" onAction="#onApplyAndCloseButtonClick" text="Apply and Close" />
				<Button fx:id="CloseButton" mnemonicParsing="false" onAction="#onCloseButtonClick" text="Close" />
			</buttons>
			<padding>
				<Insets right="10.0" />
			</padding>
		</ButtonBar>
	</children>
</VBox>
